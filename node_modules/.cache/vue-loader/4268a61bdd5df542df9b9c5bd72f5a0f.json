ewIndex }) {\n      this.handleList.push(`${src} => ${target}, ${oldIndex} => ${newIndex}`)\n    }\n  },\n  mounted () {\n    getDragList().then(res => {\n      this.list1 = res.data\n      this.list2 = [res.data[0]]\n    })\n  }\n}\n</script>\n<style lang=\"less\">\n.drag-box-card{\n  display: inline-block;\n  width: 600px;\n  height: 560px;\n  .drag-item{\n    margin: 10px;\n  }\n  h3{\n    padding: 10px 15px;\n  }\n  .drop-box{\n    border: 1px solid #eeeeee;\n    height: 455px;\n    border-radius: 5px;\n  }\n  .left-drop-box{\n    margin-right: 10px;\n  }\n  .right-drop-box{\n    //\n  }\n}\n.handle-log-box{\n  display: inline-block;\n  margin-left: 20px;\n  border: 1px solid #eeeeee;\n  vertical-align: top;\n  width: 200px;\n  height: 500px;\n  h3{\n    padding: 10px 14px;\n  }\n  .handle-inner-box{\n    height: ~\"calc(100% - 44px)\";\n    overflow: auto;\n    p{\n      padding: 14px 0;\n      margin: 0 14px;\n      border-bottom: 1px dashed #eeeeee;\n    }\n  }\n}\n.res-show-box{\n  display: inline-block;\n  margin-left: 20px;\n  border: 1px solid #eeeeee;\n  vertical-align: top;\n  width: 350px;\n  height: 570px;\n}\n</style>\n"]}]}